import java.util.Scanner;
class Number
{
	public static int sumDigit(int n)
	{
		int sum=0;
		//int n=this.n1;
		while(n>0)
		{
			sum=sum+n%10;
			n=n/10;
		}//while
			return sum;
	}//sumDigit
