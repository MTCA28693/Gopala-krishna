import java.util.*;
public class Program302
{
	static int isEven(int n)
	{
		return(n&1);
	}
	public static void main(String[] args) 
	{
		Scanner sc=new Scanner(System.in);
		int n;
		System.out.println("Enter the number");
		n=sc.nextInt();
		if(is Even(n)==0)
			System.out.println("Even\n");
		else
			System.out.println("Odd\n");
	}
}
		
		

