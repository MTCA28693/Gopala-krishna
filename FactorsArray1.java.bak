import java.util.*;
class Factors
{
	public static void main(String[] args) 
	{
		Scanner sc=new Scanner(System.in);
		int num;
		System.out.print("\n Enter a Number:");
		num=sc.nextInt();
		//int size=(int)num/2;
		//int [] FA=new int[size];
		int i,count=0;
		System.out.print("\n The factors of "+num+" are:");
		for(i=1;i<=num;i++)
		{
			if(num%i==0)
			{
				//FA[count]=i;
				count=count+1;
				System.out.print(i+" ");
			}
		}
		System.out.print("\n Total factors of "+num+" is "+count+":");
	}
}
